{
  "parser": "babel-eslint",
  "parserOptions": {
    "sourceType": "module",
    "ecmaVersion": 6,
    "ecmaFeatures": {
      "jsx": true,
      "experimentalObjectRestSpread": true
    }
  },
  "env": {
    "node": true,
    "jasmine": true,
    "es6": true,
    "browser": true
  },
  "plugins": [
    "react"
  ],
  "rules": {
    "semi": "warn",
    "no-multiple-empty-lines": "warn",
    "camelcase": "error",
    "prefer-spread": "warn",
    "no-undef-init": "error",
    "no-undef": "error",
    "no-unreachable": "error",
    "no-extra-semi": "warn",
    "no-extra-parens": "warn",
    "comma-style": "warn",
    "comma-dangle": "warn"
  }
}